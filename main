def list_to_string():

    global display_string1
    global display_string2
    global display_string3
    global display_string4
    global display_string5

    for x in display_list1:
        display_string1 = display_string1 + x

    for x in display_list2:
        display_string2 = display_string2 + x

    for x in display_list3:
        display_string3 = display_string3 + x

    for x in display_list4:
        display_string4 = display_string4 + x

    for x in display_list5:
        display_string5 = display_string5 + x


my_string = input()
word_chopper()
small_to_big_letters()
list_to_string()
display_screen()
